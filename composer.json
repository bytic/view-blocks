{
  "name": "bytic/view-blocks",
  "description": "View Blocks library for bytic",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\ViewBlocks\\": "src",
      "Nip\\Helpers\\": "src/helpers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\ViewBlocks\\Tests\\": "tests/src",
      "Nip\\ViewBlocks\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^8.0",
    "bytic/cache": "^1.0|^2.0",
    "bytic/filesystem": "^1.0|^2.0",
    "bytic/helpers": "^1.0|^2.0",
    "bytic/orm": "^1.0|^2.0",
    "bytic/view": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
