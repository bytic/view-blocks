{
  "name": "bytic/view-blocks",
  "description": "View Blocks library for bytic",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\ViewBlocks\\": "src",
      "Nip\\Helpers\\": "src/helpers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\ViewBlocks\\Tests\\": "tests/src",
      "Nip\\ViewBlocks\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=5.5",
    "bytic/cache": "^1.0",
    "bytic/filesystem": "^1.0",
    "bytic/helpers": "^1.0",
    "bytic/orm": "^1.0",
    "bytic/view": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  }
}
